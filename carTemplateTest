ele.outerFind('a.sds-link--ext').each(function(){
  let context = $ae(this).attr('aria-label');
  if(context == '' || context == undefined) {
    
  } else {
    let arr = context.split(',');
    context = arr[0];
    $ae(this).attr('aria-label', context);
  }
  $ae(this).attr('aria-describedby', 'audioeye_new_window_message');
});

