$ae(".category-link.js-news-nav-dropdown-link").each(function() {
  var currentDropDown = $ae(this);
  setTimeout(function() {
    if($ae(".js-news-nav-dropdown-content").hasClass("dropdown-hidden")){
      $ae(currentDropDown).attr("aria-expanded","false");
    }
    if(!$ae(".js-news-nav-dropdown-content").hasClass("dropdown-hidden")){
      $ae(currentDropDown).attr("aria-expanded","true");
    }
  }, 550);

  $ae(".category-link.js-news-nav-dropdown-link").click(function() {
  setTimeout(function() {
    if($ae(".js-news-nav-dropdown-content").hasClass("dropdown-hidden")){
      $ae(currentDropDown).attr("aria-expanded","false");
    }
    if(!$ae(".js-news-nav-dropdown-content").hasClass("dropdown-hidden")){
      $ae(currentDropDown).attr("aria-expanded","true");
    }
  }, 550);
  });
});


