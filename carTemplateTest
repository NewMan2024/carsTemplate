// AEI-139005, AEI-139006
ele.outerFind('.cui-accordion .cui-accordion-section').each(function() {
  var accs = $ae(this).find('label.cui-accordion-section__header');
  var accLinks = $ae(this).find('.cui-accordion-section__content a');
  ariaExpand(accs);
  api.hideFromAT(accLinks);
  $ae(this).attr('role', 'application');

  accs.click(function() {
    if ($ae(this).attr('aria-expanded') === 'true') {
      accLinks.removeAttr('role aria-hidden tabindex');
    } else {
      api.hideFromAT(accLinks);
    }
  });
});
