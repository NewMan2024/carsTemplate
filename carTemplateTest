// AEI-138864 Account button
ele.outerFind('header label#profile_menu-avatar').each(function() {
  $ae(this).attr('aria-label', 'Account');
  kbClickBtn($ae(this));
  ariaExpand($ae(this));

  $ae(this).keydown(function(e) {
    if (e.keyCode === 9 && $ae(this).attr('aria-expanded') === 'true')  {
      setTimeout(function() {
        AudioEye.focusElement('a.js_continue[data-linkname="headerpro-sign-up"]');
      }, 200);
    }
  });
});
