//////// FORM ERRORS //////// 
// AEI-138920, AEI-138923 Search error/success message
ele.outerFind('#article-search-form .article-error').attr('role', 'alert');
// AEI-138921 Email error message
ele.outerFind('cars-email-capture-form .warningAlert, cars-email-capture-form .successAlert').attr('role', 'alert');
// AEI-139018 Error focus + association
ele.outerFind('form.vv-lookup-form, form.vv-estimate-form').submit(function() {
  var errormsgs = $ae(this).find('.field p.error');
  errormsgs.each(function() {
    if (!$ae(this).attr('id')) {
      $ae(this).attr('id', 'ae_id' + Math.floor(Math.random() * 1000));
    }
  });
  $ae(this).find('input, select').each(function() {
    $ae(this).attr('aria-describedby', $ae(this).next('.error').attr('id'));
  });

  AudioEye.focusElement(errormsgs.eq(0).prev('input, select'));
});
// AEI-139030
ele.outerFind('.lead-form-container button[type="submit"]').click(function() {
  var errormsgs = $ae('.lead-form-container .input-wrapper.error .error-text');
  errormsgs.each(function() {
    $ae(this).attr('id', 'ae_id' + Math.floor(Math.random() * 1000));
    $ae(this).prev('input').attr('aria-describedby', $ae(this).attr('id'));
  });

  AudioEye.focusElement(errormsgs.eq(0).prev('input'));
});


