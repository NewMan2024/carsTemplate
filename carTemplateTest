  $ae(this).find('label').eq(0).attr({'aria-checked': 'true', 'tabindex': '0'});
  $ae(this).find('label').not(':eq(0)').attr({'aria-checked': 'false', 'tabindex': '-1'});

  AudioEye.arrowKeyLoop(ele, $ae(this).find('label'), $ae(this));

  $ae(this).prev('label').attr('id', 'ae_id' + Math.floor(Math.random() * 1000));
});
