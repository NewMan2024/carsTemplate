                    <script type="text/javascript">

                    var f = function (ele, api) {
                      // button functionality for open and close
ele.outerFind('.c-launcher iframe').contents().find('.frame-root .frame-content > div').attr({'role':'button','tabindex':'0','aria-label':'open chat'});

const myevent = new MouseEvent("click", {
  view: window,
  bubbles: true,
  cancelable: true,
});
// api.addKeyboardClick($ae('.c-launcher iframe').contents().find('.frame-root .frame-content > div'));
ele.outerFind('.c-launcher iframe').contents().find('.frame-root .frame-content > div').on('keypress',function(e){
  const closeMe = this;
  if(e.which === 32 || e.which === 13){
    setTimeout(function(){
      closeMe.dispatchEvent(myevent)
    },350)
  }
});
// hide message iframe until needed
ele.outerFind('.Messenger__MessengerFrame-sc-14laxxi-2').attr(api.rmv);

function removeIframe(iframe){
  // placeholder message iframe
  ele.outerFind('.c-proactive-exit-done iframe').attr({'role':'presentation', 'aria-hidden':'true', 'tabindex':'-1'});
  iframe.attr({'role':'presentation', 'aria-hidden':'true', 'tabindex':'-1'});
  ele.outerFind('body').data('ae-refocus-el').focus();
  iframe.contents().find('div.frame-content').removeAttr('role');
}
ele.outerFind('.c-launcher iframe').contents().find('.frame-root .frame-content > div').on('click',function(){
  $ae('body').data('ae-refocus-el', $ae(this));
  let chatIframe = $ae('.Messenger__MessengerFrame-sc-14laxxi-2');
  chatIframe.removeAttr('role tabindex aria-hidden');
  setTimeout(function(){
    // elements is not present on page load
    chatIframe.contents().find('.Styled__CloseButton-sc-17owh2b-0 > svg').attr({'role':'button', 'aria-label':'close', 'tabindex':'0'});
    chatIframe.contents().find('div.frame-content').attr('role','dialog');
    chatIframe.contents().find('.Styled__CloseButton-sc-17owh2b-0 > svg').focus();
    // keypress to mimic click event and click event
    chatIframe.contents().find('.Styled__CloseButton-sc-17owh2b-0 > svg').on('keypress',function(e){
      const closeMe = this;
      if(e.which === 32 || e.which === 13){
        setTimeout(function(){
          closeMe.dispatchEvent(myevent)
        },350)
      } else if(e.which === 9){
        console.log('tab')
        setTimeout(function(){
          console.log('focusout first');
          console.log('shift key');
          this.focus();
        },350)
      }
    });
    chatIframe.contents().find('.Styled__CloseButton-sc-17owh2b-0 > svg').on('click',function(){
      setTimeout(function(){
        // hides frame when not active
        removeIframe(chatIframe);
      }, 750);
    })
    .on('keydown',function(e){
      if(e.shiftKey === true && e.which === 9){
        setTimeout(function(){
          chatIframe.contents().find('textarea, a, button').last().focus();
        },350);
      }
    });
    chatIframe.contents().find('textarea, a, button').last().on('keydown',function(e){
      if(e.shiftKey === false && e.which === 9){
        setTimeout(function(){
          chatIframe.contents().find('.Styled__CloseButton-sc-17owh2b-0 > svg').focus();
        },350);
      }
    });
  }, 750);
});


