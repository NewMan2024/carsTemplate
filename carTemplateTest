var refinementsCatConfig = { attributes: true, attributeFilter: ["class"] };
var refinementsCatObserver = new MutationObserver(function (mutation) {
  mutation.forEach(function (mutation) {
    $ae('.article-page .sds-template-sidebar .sds-template-sidebar__content .author-content-container .social-buttons .sds-social-share.sds-drawer--expanded .sds-drawer-extra .sds-drawer__item--trigger a').attr('aria-expanded', 'true');
