      event.preventDefault();
      $ae(this)[0].click();
    }
  });
  $ae(this).attr('alt', '');
  $ae(this).parent().parent().prev('div').removeAttr('tabindex');
});

//IM-12900 AT: Home Page: Missing or confusing regions.
